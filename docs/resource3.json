  "txt": [
   "GOVERNO DO ESTADO\n",
   "\n",
   "PERNAMBUCO\n",
   "\n",
   "MAIS TRABALHO, MAIS FUTURO\n",
   "\n",
   "Secretaria de\n",
   "Sa\u00fade\n",
   "\n",
   " \n",
